{
    "compilerOptions": {
      "jsx": "react",
      "moduleResolution": "node",
      "target": "es6", 
      "module": "es6", 
      "outDir": "./dist", 
      "strict": true, 
      "esModuleInterop": true, 
      "forceConsistentCasingInFileNames": true, 
      "skipLibCheck": true, 
      "baseUrl": ".", 
      "paths": {
        "redux": ["node_modules/redux"]
      }
    },
    "include": ["src"], 
    "exclude": ["node_modules"] 
  }
  